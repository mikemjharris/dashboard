{
  "name": "Dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./server/server.js",
    "build-prod": "NODE_ENV=production webpack --config webpack/prod.config.js && cp ./server/views/index.html ./dist && cp ./public/*.* ./dist",
    "all": "webpack && node ./server/server.js",
    "start-dev": "node ./server/server.js | NODE_ENV=development webpack --config webpack/dev.config.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.1",
    "cookie-parser": "^1.4.3",
    "debug": "^4.0.1",
    "ejs": "^2.5.8",
    "express": "~4.17.3",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.6",
    "path": "^0.12.7",
    "static-favicon": "~1.0.0",
    "vue": "^2.5.16",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.2"
  }
}
